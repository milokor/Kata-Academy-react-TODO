{
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "import/prefer-default-export": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  